{
  "name": "mangga",
  "version": "0.0.1",
  "description": "A manga downloader CLI tool.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && babel --source-maps inline --copy-files src --out-dir lib",
    "test": "node test | tap-spec",
    "test:babel": "babel-node test | tap-spec"
  },
  "author": "Richard Neil Ilagan <me@richardneililagan.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bluebird": "3.2.2",
    "bson": "0.4.21",
    "chalk": "1.1.1",
    "cheerio": "0.20.0",
    "got": "6.1.1",
    "lodash": "4.3.0",
    "meow": "3.7.0",
    "observatory": "1.0.0",
    "pouchdb": "5.2.1",
    "throat": "2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.0",
    "babel-plugin-transform-es2015-literals": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "babel-plugin-transform-strict-mode": "^6.5.0",
    "eslint": "1.10.3",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-standard": "1.3.2",
    "tap-spec": "4.1.1",
    "tape": "4.4.0",
    "tape-eslint": "1.2.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-arrow-functions",
      "transform-es2015-literals",
      "transform-es2015-parameters",
      "transform-strict-mode"
